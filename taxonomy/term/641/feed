&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;Я не искала - сам меня нашел! -&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;Взамен тебя теперь Вселенной мало!!!&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;С тобой всегда мне просто хорошо!&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;Так классно, что тебя я повстречала!&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt; &lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;Но жизнь - коварная плутовка, -&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;Людей похожих сводит ловко!&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;Да только лбами и в разнос!!!&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;И слишком души были близко!&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;Но ты так испугался риска!&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;color:#003300&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;em&gt;&lt;span style=&quot;font-size:18px&quot;&gt;И не рискнув мечту унёс…..&lt;/span&gt;&lt;/em&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;
