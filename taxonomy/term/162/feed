&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Прикольные смс с днём молодежи&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt; &lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Нет молодежи на свете!&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Есть – постаревшие дети!&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Бабушки дедушки наши -&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Все в глубине первоклашки!&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt; &lt;/p&gt;

&lt;p&gt; &lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Мы еще не молодежь,&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Но по взрослому гуляем.&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Петь Алин, Светлан, Сереж –&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;С днем молодежи поздравляем!&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt; &lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;С днем молодежи дружище!&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Пусть будет жизнь офигенной!&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Пусть будут сотнями тыщи!&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p&gt;&lt;span style=&quot;font-size:18px&quot;&gt;&lt;span style=&quot;font-family:georgia,serif&quot;&gt;&lt;strong&gt;&lt;em&gt;&lt;span style=&quot;color:#000000&quot;&gt;Ну а здоровье железным!&lt;/span&gt;&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
